#ALIAS
alias fastfetch="fastfetch --config $HOME/.config/fastfetch/config"
alias ll="ls -alh"
export EDITOR=vim

export PATH="$HOME/mpvpaper/build:$PATH"
export PATH=$PATH:/usr/local/go/bin

source /usr/share/nvm/init-nvm.sh
export PATH="$HOME/.cargo/bin:$PATH"
export RUSTUP_HOME="$HOME/.rustup"
export CARGO_HOME="$HOME/.cargo"
export PATH="$CARGO_HOME/bin:$PATH"
export TERM=xterm-256color

#xhost +SI:localuser:root

export PATH=$HOME/.local/bin:$PATH

export BAT_THEME="Dracula"

. $HOME/.rsvm/current/cargo/env

export PYENV_ROOT="$HOME/.pyenv"
[[ -d $PYENV_ROOT/bin ]] && export PATH="$PYENV_ROOT/bin:$PATH"


export NVM_DIR="$HOME/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && \. "$NVM_DIR/nvm.sh" # This loads nvm
[ -s "$NVM_DIR/bash_completion" ] && \. "$NVM_DIR/bash_completion"  # This loads nvm bash_completion


export PATH="/opt/winbox/:$PATH"
alias winbox="wine /opt/winbox/winbox64.exe"

export CHROME_EXECUTABLE=/usr/bin/chromium
alias neofetch="neofetch --config ~/neofetch-themes/small/simplefetch.conf"
export ANDROID_HOME=$HOME/Android/Sdk

export QT_QPA_PLATFORM=wayland
export MOZ_ENABLE_WAYLAND=1
export PATH=$HOME/.config/composer/vendor/bin:$PATH
export JAVA_HOME=/usr/lib/jvm/default
export PATH=$JAVA_HOME/bin:$PATH
export PATH=$HOME/flutter/bin:$PATH
export PATH=$ANDROID_HOME/cmdline-tools/latest/bin:$PATH
export PATH=/home/syafiq/bin:$PATH
export PATH=$HOME/.pub-cache/bin:$PATH
export PATH=$ANDROID_HOME/platform-tools:$PATH
export DOCKER_HOST=unix:///run/user/1000/docker.sock


